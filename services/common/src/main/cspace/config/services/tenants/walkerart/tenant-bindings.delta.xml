<?xml version="1.0" encoding="UTF-8"?>
<tenant:TenantBindingConfig
    xmlns:merge='http://xmlmerge.el4j.elca.ch'
    xmlns:tenant='http://collectionspace.org/services/config/tenant'>
             
    <tenant:tenantBinding id="5" name="walkerart.org" displayName="CollectionSpace Walker Art Center Tenant" version="0.1">
        <tenant:repositoryDomain name="default-domain" storageName="walkerart-domain" repositoryClient="nuxeo-java" />

        <tenant:serviceBindings merge:matcher="id" id="CollectionObjects">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="collectionobjects_variablemedia" updated="" order="3">
                    <!--service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>fieldName</types:value>
                        </types:item>
                    </service:properties-->
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/collectionobject/domain/variablemedia" 
                            schemaLocation="http://collectionspace.org/services/collectionobject/domain/variablemedia http://collectionspace.org/services/collectionobject/domain/collectionobjects_variablemedia.xsd" />
                    </service:content>
                </service:part>

                <service:part id="4" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="collectionobjects_walkerart" updated="" order="4">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>collectionsWAC|collectionWAC</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>materialGroupList/*/materialComponent</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>objectComponentGroupList/*/objectComponentName</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/collectionobject/local/walkerart" schemaLocation="http://collectionspace.org/services/collectionobject/local/walkerart http://collectionspace.org/services/collectionobject/local/collectionobjects_walkerart.xsd" />
                    </service:content>
                </service:part>

            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Media">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="media_walkerart" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>lastModifiedByWAC</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/media/local/walkerart" schemaLocation="http://collectionspace.org/services/media/local/walkerart http://collectionspace.org/services/media/local/media_walkerart.xsd" />
                    </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Movements">
            <service:object xmlns:service="http://collectionspace.org/services/config/service">
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="movements_walkerart" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>onViewLocationWAC</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/movement/local/walkerart" schemaLocation="http://collectionspace.org/services/movement/local/walkerart http://collectionspace.org/services/movement/local/movements_walkerart.xsd" />
                    </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Persons">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="persons_walkerart" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>associationWAC</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/person/local/walkerart" 
                            schemaLocation="http://collectionspace.org/services/person/local/walkerart http://collectionspace.org/services/person/local/persons_walkerart.xsd" />
                    </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Acquisitions">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="acquisitions_walkerart" updated="" order="3">
                    <service:properties>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>insuredByGroupWAC|insuredByWAC</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>acquisitionFundingWACList/*/acquisitionFundingSourceWAC</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>acquisitionFundingWACList/*/acquisitionFundingCurrencyWAC</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/acquisition/local/walkerart" schemaLocation="http://collectionspace.org/services/acquisition/local/walkerart http://collectionspace.org/services/acquisition/local/acquisitions_walkerart.xsd" />
                     </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

        <tenant:serviceBindings merge:matcher="id" id="Contacts">
            <service:object xmlns:service="http://collectionspace.org/services/config/service" >
                <service:part id="3" merge:matcher="skip" merge:action="insert" control_group="Managed" versionable="true" auditable="false" label="contacts_walkerart" updated="" order="3">
                    <service:properties>
                        <!--types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>addressGroupListWAC/*/addressMunicipalityWAC</types:value>
                        </types:item-->
                        <!--types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>authRef</types:key>
                            <types:value>addressGroupListWAC/*/addressStateOrProvinceWAC</types:value>
                        </types:item-->
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>addressGroupListWAC/*/addressCountryWAC</types:value>
                        </types:item>
                        <types:item xmlns:types="http://collectionspace.org/services/config/types">
                            <types:key>termRef</types:key>
                            <types:value>addressGroupListWAC/*/addressTypeWAC</types:value>
                        </types:item>
                    </service:properties>
                    <service:content contentType="application/xml">
                        <service:xmlContent namespaceURI="http://collectionspace.org/services/contact/local/walkerart" 
                            schemaLocation="http://collectionspace.org/services/contact/local/walkerart http://collectionspace.org/services/contact/local/contacts_walkerart.xsd" />
                    </service:content>
                </service:part>
            </service:object>
        </tenant:serviceBindings>

    </tenant:tenantBinding>
                   
</tenant:TenantBindingConfig>